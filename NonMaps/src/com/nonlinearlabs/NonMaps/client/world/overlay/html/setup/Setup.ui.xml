<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<g:HTMLPanel>
		<div class="tabcontainer w33 html-dialog">

			<div class="tab">
				<g:Button styleName="tabheader" ui:field="deviceSettingsButton">Device Settings
				</g:Button>
				<g:Button styleName="tabheader" ui:field="uiSettingsButton">GUI Settings
				</g:Button>
				<g:Button styleName="tabheader" ui:field="systemInfoButton">System Info
				</g:Button>
				<g:Button styleName="tabheader" ui:field="aboutButton">About</g:Button>
			</div>

			<div ui:field="deviceSettings" class="tabcontent">
				<div class="forms-container">
					<div class="mt1"></div>
					<div class="eins-he">
						<div class="w70p">
							<p>Velocity Curve</p>
						</div>
						<div class="w30p">
							<g:ListBox ui:field="velocityCurve">
							</g:ListBox>
						</div>
					</div>
					<div class="linie"></div>

					<div class="eins-he">
						<div class="w70p">
							<p>Aftertouch Curve</p>
						</div>
						<div class="w30p">
							<g:ListBox ui:field="aftertouchCurve">
							</g:ListBox>
						</div>
					</div>

					<div class="linie"></div>

					<div class="eins-he">
						<div class="w70p">
							<p>Bender Curve</p>
						</div>
						<div class="w30p">
							<g:ListBox ui:field="benderCurve">
							</g:ListBox>
						</div>
					</div>

					<div class="linie"></div>

					<div class="eins-he fl-j-center">
						<h3 class="uneditable">Pedal Settings</h3>
					</div>

					<div class="eins-he">
						<div class="w30p">
							<p>Pedal 1</p>
						</div>
						<div class="w10p">
							<g:Label ui:field="pedal1DisplayString" styleName="gui-label-1">90 %</g:Label>
						</div>
						<div class="w25p">
							<input type="range" ui:field="pedal1Slider" class="slider-b"
								min="0" max="1" step="0.001" disabled="disabled"></input>
						</div>
						<div class="w5p"></div>
						<div class="w30p">
							<g:ListBox ui:field="pedal1Type">
							</g:ListBox>
						</div>
					</div>

					<div class="eins-he">
						<div class="w30p">
							<p>Pedal 2</p>
						</div>
						<div class="w10p">
							<g:Label ui:field="pedal2DisplayString" styleName="gui-label-1">90 %</g:Label>
						</div>
						<div class="w25p">
							<input type="range" ui:field="pedal2Slider" class="slider-b"
								min="0" max="1" step="0.001" disabled="disabled"></input>
						</div>
						<div class="w5p"></div>
						<div class="w30p">
							<g:ListBox ui:field="pedal2Type">
							</g:ListBox>
						</div>
					</div>

					<div class="eins-he">
						<div class="w30p">
							<p>Pedal 3</p>
						</div>
						<div class="w10p">
							<g:Label ui:field="pedal3DisplayString" styleName="gui-label-1">90 %</g:Label>
						</div>
						<div class="w25p">
							<input type="range" ui:field="pedal3Slider" class="slider-b"
								min="0" max="1" step="0.001" disabled="disabled"></input>
						</div>
						<div class="w5p"></div>
						<div class="w30p">
							<g:ListBox ui:field="pedal3Type">
							</g:ListBox>
						</div>
					</div>

					<div class="eins-he">
						<div class="w30p">
							<p>Pedal 4</p>
						</div>
						<div class="w10p">
							<g:Label ui:field="pedal4DisplayString" styleName="gui-label-1">90 %</g:Label>
						</div>
						<div class="w25p">
							<input type="range" ui:field="pedal4Slider" class="slider-b"
								min="0" max="1" step="0.001" disabled="disabled"></input>
						</div>
						<div class="w5p"></div>
						<div class="w30p">
							<g:ListBox ui:field="pedal4Type">
							</g:ListBox>
						</div>
					</div>

					<div class="linie"></div>

					<div class="eins-he">
						<div class="w70p">
							<p>Preset Glitch Surpression</p>
						</div>
						<div class="w30p mw-fit-content">
							<g:RadioButton ui:field="presetGlitchSurpressionOn"
								name="rad_pgs" value="true"></g:RadioButton>
							<g:RadioButton ui:field="presetGlitchSurpressionOff"
								name="rad_pgs" value="false"></g:RadioButton>
						</div>
					</div>

					<div class="linie"></div>

					<div class="eins-he">
						<div class="w35p">
							<p>Edit Smoothing Time</p>
						</div>
						<div class="w20p">
							<g:Label ui:field="editSmoothingTimeDisplayString"
								styleName="gui-label-1">90 %</g:Label>
						</div>
						<div class="w40p">
							<input type="range" ui:field="editSmoothingTimeSlider"
								class="slider-a" min="0" max="1" step="0.001" value="0"></input>
						</div>
					</div>

				</div>
			</div>

			<div ui:field="uiSettings" class="tabcontent">

				<div class="forms-container">
					<div class="mt1"></div>

					<div class="eins-he">
						<div class="w60p">
							<p>Selection Auto Scroll</p>
						</div>
						<div class="w40p">
							<g:ListBox ui:field="selectionAutoScroll">
							</g:ListBox>
						</div>
					</div>

					<div class="linie"></div>

					<div class="eins-he">
						<div class="w60p">
							<p>Edit Parameter</p>
						</div>
						<div class="w40p">
							<g:ListBox ui:field="editParameter">
							</g:ListBox>
						</div>
					</div>

					<div class="linie"></div>

					<div class="eins-he">
						<div class="w60p">
							<p>Show Context Menus</p>
						</div>
						<div class="w40p mw-fit-content">
							<g:RadioButton ui:field="showContextMenusOn" name="rad_scm"
								value="true"></g:RadioButton>
							<g:RadioButton ui:field="showContextMenusOff" name="rad_scm"
								value="false"></g:RadioButton>
						</div>
					</div>

					<div class="linie"></div>

					<div class="eins-he">
						<div class="w60p">
							<p>Preset Drag and Drop</p>
						</div>
						<div class="w40p mw-fit-content">
							<g:RadioButton ui:field="presetDragDropOn" name="rad_pdd"
								value="true"></g:RadioButton>
							<g:RadioButton ui:field="presetDragDropOff" name="rad_pdd"
								value="false"></g:RadioButton>
						</div>
					</div>

					<div class="linie"></div>

					<div class="eins-he">
						<div class="w60p">
							<p>Display Scaling Factor</p>
						</div>

						<div class="w40p">
							<g:ListBox ui:field="scalingFactor">
							</g:ListBox>
						</div>
					</div>

					<div class="linie"></div>

					<div class="eins-he">
						<div class="w60p">
							<p>Stripe Brightness</p>
						</div>

						<div class="w40p">
							<g:ListBox ui:field="stripeBrightness">
							</g:ListBox>
						</div>
					</div>

					<div class="linie"></div>

					<div class="eins-he">
						<div class="w60p">
							<p>Bitmap Cache</p>
						</div>
						<div class="w40p mw-fit-content">
							<g:RadioButton ui:field="bitmapCacheOn" name="rad_bmpc"
								value="true"></g:RadioButton>
							<g:RadioButton ui:field="bitmapCacheOff" name="rad_bmpc"
								value="false"></g:RadioButton>
						</div>
					</div>

					<div class="linie"></div>

					<div class="eins-he">
						<div class="w60p">
							<p>Show Developer Options</p>
						</div>
						<div class="w40p mw-fit-content">
							<g:RadioButton ui:field="developerOptionsOn" name="rad_devop"
								value="true"></g:RadioButton>
							<g:RadioButton ui:field="developerOptionsOff" name="rad_devop"
								value="false"></g:RadioButton>
						</div>
					</div>

				</div>
			</div>


			<div ui:field="systemInfo" class="tabcontent">

				<div class="forms-container">

					<div class="mt1"></div>

					<div class="eins-he">
						<div class="w40p">
							<p>Device Name</p>
						</div>
						<div class="w40p">
							<g:TextArea ui:field="deviceName" styleName="editable w100p txt-oneline" visibleLines="1"></g:TextArea>
						</div>
						<div class="w5p"></div>
						<div class="w15p fl-j-center">
							<g:Button ui:field="saveDeviceName" text="Save"></g:Button>
						</div>
					</div>

					<div class="eins-he">
						<div class="w40p">
							<p>Free Internal Memory</p>
						</div>
						<div class="w60p">
							<g:Label ui:field="freeMemory"></g:Label>
						</div>
					</div>

					<div class="linie"></div>

					<div class="mt05"></div>

					<div class="halb-he">
						<div class="w40p">
							<p>UI Software Version</p>
						</div>
						<div class="w60p">
							<g:Label ui:field="uiVersion"></g:Label>
						</div>
					</div>

					<div class="halb-he">
						<div class="w40p">
							<p>RT Software Version</p>
						</div>
						<div class="w60p">
							<g:Label ui:field="rtVersion"></g:Label>
						</div>
					</div>

					<div class="halb-he">
						<div class="w40p">
							<p>OS Version</p>
						</div>
						<div class="w60p">
							<g:Label ui:field="osVersion"></g:Label>
						</div>
					</div>
					
					<div class="halb-he">
						<div class="w40p">
							<p>Date/Time</p>
						</div>
						<div class="w60p">
							<g:Label ui:field="dateTime"></g:Label>
						</div>
					</div>

					<div class="mb05"></div>

				</div>
			</div>



			<div ui:field="about" class="tabcontent">

				<div class="forms-container">

					<div class="mt1"></div>

					<h2 class="h-blau">C15 Development &amp; Design Team</h2>
					<div class="txt-area">

						<p>Stephan Schmitt,
							Daniel Tzschentke,
							Simone Fabbri,
							Henry Högelow,
							Matthias Seeber,
							Pascal Hürst,
							Tino Meinert,
							Gabriel Mulzer,
							Mike
							Daliot,
							Justus Scheil,
							Nimanija Nikodijevic,
							Ulrich Wappler,
							Anton
							Schmied,
							Brian Smith,
							Klaus Strohhäcker,
							Paul Schulz
						</p>
					</div>

					<div class="t-center mt2 mb1 h-blau">
						<small>
							<p>
								NONLINEAR LABS GmbH
								<br />
								Helmholtzstraße 2-9 E
								<br />
								10587
								Berlin Germany
							</p>
							<p>
								www.nonlinear-labs.de
								<br />
								E-Mail: info@nonlinear-labs.de
							</p>
						</small>
					</div>

				</div>
			</div>


		</div>
	</g:HTMLPanel>
</ui:UiBinder>